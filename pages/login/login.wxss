/* pages/login/login.wxss */
.input{
    padding-left: 10px;
    height: 44px;
}
.inputView{
    /*边界：大小1px, 为固体，为绿色*/
    border:  1px solid green;
    /*边界角的弧度*/
    border-radius: 30px;
    margin-left: 15px;
    margin-right: 15px;
    margin-top: 15px;
}
.buttom{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 1.5em;
}
.forgetPassword{
  color: #3622e9;
  position: absolute;
  margin-left: 1em;
  margin-top: 1.5em;
}
.register{
  color: #3622e9;
  position: absolute;
  margin-left: 15em;
  margin-top: 1.5em;
}