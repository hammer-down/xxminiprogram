<!--pages/personal/set-up/set-up.wxml-->
<import src="../option-template/option-template.wxml" />
<view class="placeholder"></view>

<view class="container">

  <view class="page_title">
    <image bindtap="goBack" class="back" src="/img/back.png"></image>
    <view class='info'>
      <text>设置</text>
    </view>
  </view>
</view>

<view class='item'>
  <view class="clearStorage" bindtap='clearCacheTap'>
    <template is='optionTemplate' data="{{name:clearStorage}}" />
  </view>
  <template is='optionTemplate' data="{{name:update}}" />
</view>